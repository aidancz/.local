# "a <- b" means a is linked by b
# "a << b" means a is referenced by b



# foo's config is effective at ~/.config/foo
.config/foo <- ~/.config/foo

# foo's config is not effective at ~/.config/foo
.config/.foo <- ~/.config/.foo

~/.zprofile -> .config/.sh/profile >> .config/.x11/xinitrc >> ~/.xprofile -> .config/.x11/xprofile
                                                           >> dwm
